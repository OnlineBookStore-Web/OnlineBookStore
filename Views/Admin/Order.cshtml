@model List<OnlineBookStore.Models.OrderDetail>

<h3>Orders Management</h3>
<table class="table table-bordered table-striped table-hover">
    <thead class="table-dark">

        <tr>
            <th>ID</th>
            <th>User</th>
            <th>Book</th>
            <th>Quantity</th>
            <th>Total Price</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var order in Model)
        {
            <tr>
                <td>@order.OrderID</td>
                <td>@order.UserID</td>
                <td>@order.BookID</td>
                <td>@order.Quantity</td>
                <td>@order.Price</td>
            </tr>
        }
    </tbody>
</table>
