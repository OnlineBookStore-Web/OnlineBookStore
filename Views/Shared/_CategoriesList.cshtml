@model IEnumerable<OnlineBookStore.Models.Category>

<div class="list-group">
    <a href="/Books" class="list-group-item">All Books</a>

    @foreach (var c in Model)
    {
        <a href="/Books?categoryId=@c.CategoryId"
           class="list-group-item list-group-item-action">
            @c.Name
        </a>
    }
</div>
